   "source": [
    "import joblib\n",
    "\n",
    "# carrega o mesmo modelo usado no app.py\n",
    "model = joblib.load(\"tfidf_logreg_model.pkl\")\n",
    "\n",
    "# gera predi√ß√µes para todos os coment√°rios\n",
    "y_pred = model.predict(df[\"customer_comment\"])\n",
    "\n",
    "# sobrescreve a coluna ia_ruim com o que o modelo est√° prevendo\n",
    "df[\"ia_ruim\"] = y_pred\n",
    "\n",
    "df[[\"ticket_id\", \"customer_comment\", \"rating\", \"ia_ruim\", \"humano_classe\"]].head(10)\n"
   ]
