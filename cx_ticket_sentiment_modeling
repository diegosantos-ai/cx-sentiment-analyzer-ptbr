    "# 1. IMPORTS\n",
    "import pandas as pd\n",
    "\n",
    "df_base = pd.read_csv(\"olist_order_reviews_dataset.csv.csv\")\n",
    "\n",
    "print(df_base.shape)\n",
    "df_base.head()\n",
    "\n",
    "df = pd.read_excel(\"REPORT_ROTULO_HUMANO.xlsx\")\n",
    "print(df.shape)\n",
    "print(df.head(3))\n",
    "df.to_csv(\"report_analise.csv\", index=False)  # Salva CSV backup\n",
    "\n",
