   "source": [
    "# 5. FIX: Dataset com labels L√ìGICOS (profissional)\n",
    "print(\"üîß Recriando com rating coerente...\")\n",
    "\n",
    "# REGRA BUSINESS REAL (iFood CX style)\n",
    "def assign_rating(comment):\n",
    "    comment_lower = comment.lower()\n",
    "    neg_words = ['grosseiro', 'demora', 'erro', 'ruim', 'travando', 'urgente']\n",
    "    pos_words = ['√≥timo', 'excelente', 'perfeita', 'r√°pido', 'obrigado', 'educado']\n",
    "    \n",
    "    neg_count = sum(1 for word in neg_words if word in comment_lower)\n",
    "    pos_count = sum(1 for word in pos_words if word in comment_lower)\n",
    "    \n",
    "    return 1 if pos_count > neg_count else 0  # Positivo VENCE negativo\n",
    "\n",
    "# APLICAR REGRA\n",
    "df['rating_logico'] = df['customer_comment'].apply(assign_rating)\n",
    "\n",
    "# RE-TREINAR MODELO\n",
    "df['predicted_final'] = (df['sentiment_ptbr'] >= 0).astype(int)\n",
    "\n",
    "print(\"üéØ MODEL FINAL (labels l√≥gicos):\")\n",
    "print(\"ACERTOU:\", (df['predicted_final'] == df['rating_logico']).mean().round(2))\n",
    "print(\"\\nüìä Matriz confus√£o:\")\n",
    "cm_df = pd.crosstab(df['rating_logico'], df['predicted_final'], normalize='all')\n",
    "print(cm_df.round(2))\n",
    "\n",
    "print(\"\\nüî• Top ERROS (aprender com eles):\")\n",
    "erros = df[df['rating_logico'] != df['predicted_final']]\n",
    "print(erros[['customer_comment', 'rating_logico', 'predicted_final']].head())\n"
   ]
  },
