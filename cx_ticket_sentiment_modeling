   "source": [
    "# 4.5 FIX PT-BR: Lexicon customizado brasileiro\n",
    "print(\"üáßüá∑ Lexicon PT-BR customizado...\")\n",
    "\n",
    "# PALAVRAS QUE IMPORTAM NO CX BRASILEIRO\n",
    "ptbr_positive = ['√≥timo', 'excelente', 'perfeita', 'r√°pido', 'eficiente', 'obrigado', \n",
    "                'educado', 'atencioso', 'resolvido', 'funciona', 'recomendo', 'nota 10']\n",
    "\n",
    "ptbr_negative = ['grosseiro', 'demora', 'travando', 'erro', 'ruim', 'inaceit√°vel', \n",
    "                'urgente', 'sem resposta', 'reembolso', 'compra', 'chat']\n",
    "\n",
    "def sentiment_ptbr(text):\n",
    "    text = text.lower()\n",
    "    pos_score = sum(1 for word in ptbr_positive if word in text)\n",
    "    neg_score = sum(1 for word in ptbr_negative if word in text)\n",
    "    compound = pos_score - neg_score\n",
    "    return compound / max(1, len(text.split()))  # Normaliza\n",
    "\n",
    "# APLICA NOVO MODELO\n",
    "df['sentiment_ptbr'] = df['text_clean'].apply(sentiment_ptbr)\n",
    "df['predicted_ptbr'] = (df['sentiment_ptbr'] >= 0).astype(int)\n",
    "\n",
    "print(\"üéØ NOVO MODELO PT-BR:\")\n",
    "print(\"ACERTOU:\", (df['predicted_ptbr'] == df['rating']).mean().round(2))\n",
    "print(\"\\nüìà Compara√ß√£o:\")\n",
    "print(df[['text_clean', 'rating', 'sentiment_ptbr', 'predicted_ptbr']].head(10))\n",
    "print(\"\\nüî• Top palavras que salvaram:\")\n",
    "print(\"Positivas detectadas:\", df[df['sentiment_ptbr'] > 0]['text_clean'].str.contains('|'.join(ptbr_positive)).sum())\n",
    "print(\"Negativas detectadas:\", df[df['sentiment_ptbr'] < 0]['text_clean'].str.contains('|'.join(ptbr_negative)).sum())\n"
   ]
  },
