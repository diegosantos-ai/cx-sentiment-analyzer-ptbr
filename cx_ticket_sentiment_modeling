    "# 4. MODELING: Sentimento Autom√°tico PT-BR\n",
    "print(\"ü§ñ Treinando modelo de sentimento CX...\")\n",
    "\n",
    "# 4.1 INSTALAR NLTK (1x s√≥)\n",
    "import nltk\n",
    "nltk.download('vader_lexicon', quiet=True)\n",
    "nltk.download('punkt', quiet=True)\n",
    "\n",
    "# 4.2 CARREGAR VADER (funciona PT-BR b√°sico)\n",
    "from nltk.sentiment.vader import SentimentIntensityAnalyzer\n",
    "sia = SentimentIntensityAnalyzer()\n",
    "\n",
    "# 4.3 TESTAR EM 1 TICKET\n",
    "teste = df['text_clean'].iloc[1]\n",
    "score = sia.polarity_scores(teste)\n",
    "print(\"üß™ TESTE:\", teste)\n",
    "print(\"üìä SCORE:\", score)\n",
    "print(\"VEREDICTO:\", \"üòä\" if score['compound'] >= 0 else \"üò°\")\n",
    "\n",
    "# 4.4 APLICAR EM TODOS (5s)\n",
    "df['sentiment_score'] = df['text_clean'].apply(lambda x: sia.polarity_scores(x)['compound'])\n",
    "df['predicted_rating'] = (df['sentiment_score'] >= 0).astype(int)\n",
    "\n",
    "print(\"\\nüéØ RESULTADOS MODEL:\")\n",
    "print(\"ACERTOU:\", (df['predicted_rating'] == df['rating']).mean().round(2))\n",
    "print(\"\\nüìà Amostra predi√ß√£o:\")\n",
    "print(df[['text_clean', 'rating', 'sentiment_score', 'predicted_rating']].head())\n"
