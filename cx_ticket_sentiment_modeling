   ],
   "source": [
    "# 5. DESEMPENHO POR CLASSE (0 = Bom, 1 = Ruim)\n",
    "\n",
    "resumo = (\n",
    "    df.groupby(\"humano_classe\")\n",
    "      .agg(\n",
    "          total=(\"humano_classe\", \"count\"),\n",
    "          acertos=(\"acertou\", \"sum\")\n",
    "      )\n",
    ")\n",
    "\n",
    "resumo[\"erros\"] = resumo[\"total\"] - resumo[\"acertos\"]\n",
    "resumo[\"accuracy_classe\"] = resumo[\"acertos\"] / resumo[\"total\"]\n",
    "\n",
    "resumo\n"
   ]
  },
  {
