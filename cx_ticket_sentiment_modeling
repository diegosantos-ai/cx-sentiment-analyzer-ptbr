    "# PASSO D: manter s√≥ frases com UM √∫nico r√≥tulo (0=negativo, 1=positivo)\n",
    "\n",
    "# conta quantos r√≥tulos diferentes cada frase tem\n",
    "rotulos_por_frase = (\n",
    "    df.groupby(\"customer_comment\")[\"rating\"]\n",
    "      .nunique()\n",
    "      .reset_index()\n",
    ")\n",
    "\n",
    "# frases \"saud√°veis\" = s√≥ 1 r√≥tulo\n",
    "frases_ok = rotulos_por_frase[rotulos_por_frase[\"rating\"] == 1][\"customer_comment\"]\n",
    "\n",
    "# filtra o dataframe original\n",
    "df_limpo = df[df[\"customer_comment\"].isin(frases_ok)].copy()\n",
    "\n",
    "print(\"Tamanho antigo:\", len(df))\n",
    "print(\"Tamanho ap√≥s limpeza:\", len(df_limpo))\n",
