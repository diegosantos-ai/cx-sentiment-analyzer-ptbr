   ],
   "source": [
    "# 4. CONTAGEM DE ACERTOS / ERROS\n",
    "\n",
    "# acerto = quando previs√£o da IA (ia_ruim) √© igual ao r√≥tulo humano (humano_classe)\n",
    "df[\"acertou\"] = (df[\"ia_ruim\"] == df[\"humano_classe\"]).astype(int)\n",
    "\n",
    "total = len(df)\n",
    "acertos = df[\"acertou\"].sum()\n",
    "erros = total - acertos\n",
    "acc = acertos / total\n",
    "\n",
    "print(f\"Total de tickets: {total}\")\n",
    "print(f\"Acertos: {acertos}\")\n",
    "print(f\"Erros: {erros}\")\n",
    "print(f\"Accuracy geral: {acc:.2%}\")\n"
   ]
  },
